public class Youtuber {
    //ATRIBUTOS
    private String nombreCanal;
    private String Categoría;
    private int numeroSuscriptores;

    public String getNombreCanal() {
        return nombreCanal;
    }

    public void setNombreCanal(String nombreCanal) {
        this.nombreCanal = nombreCanal;
    }

    public String getCategoría() {
        return Categoría;
    }

    public void setCategoría(String categoría) {
        Categoría = categoría;
    }

    public int getNumeroSuscriptores() {
        return numeroSuscriptores;
    }

    public void setNumeroSuscriptores(int numeroSuscriptores) {
        this.numeroSuscriptores = numeroSuscriptores;
    }
}
class youtubers {
    public static void main(String[] args) {
        Youtuber youtuber1 = new Youtuber();
        Youtuber youtuber2 = new Youtuber();
        Youtuber youtuber3 = new Youtuber();

        youtuber1.setNumeroSuscriptores(300);
        youtuber1.setCategoría("Videojuegos");
        youtuber1.setNombreCanal("Xokas");
        youtuber2.setNombreCanal("Ibai");
        youtuber2.setCategoría("Just Chatting");
        youtuber2.setNumeroSuscriptores(970);
        youtuber3.setNombreCanal("WIllyrex");
        youtuber3.setNumeroSuscriptores(1800);
        youtuber3.setCategoría("Viajes");

        System.out.println("Youtuber 1: " + youtuber1.getNombreCanal() + " - Nº Suscriptores: " + youtuber1.getNumeroSuscriptores() + " - Categoría: " + youtuber1.getCategoría());
        System.out.println("Youtuber 2: " + youtuber2.getNombreCanal() + " - Nº Suscriptores: " + youtuber2.getNumeroSuscriptores() + " - Categoría: " + youtuber2.getCategoría());
        System.out.println("Youtuber 3: " + youtuber3.getNombreCanal() + " - Nº Suscriptores: " + youtuber3.getNumeroSuscriptores() + " - Categoría: " + youtuber3.getCategoría());

        System.out.println("-----------------------");
        System.out.println("Actualización se suscriptores");

        youtuber1.setNumeroSuscriptores(320);
        youtuber2.setNumeroSuscriptores(1000);
        youtuber3.setNumeroSuscriptores(2500);

        System.out.println("Nº Suscriptores Youtuber 1: " + youtuber1.getNumeroSuscriptores());
        System.out.println("Nº Suscriptores Youtuber 2: " + youtuber2.getNumeroSuscriptores());
        System.out.println("Nº Suscriptores Youtuber 3: " + youtuber3.getNumeroSuscriptores());

    }

}
